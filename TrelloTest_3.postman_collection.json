		{
			"name": "Create several boards",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"const names = pm.collectionVariables.get(\"names\");\r",
							"console.log(names);\r",
							"console.log(names.length);\r",
							"if (names.length > 0){\r",
							"    postman.setNextRequest(\"Create several boards\");\r",
							"} else {\r",
							"    postman.setNextRequest(null);\r",
							"    pm.collectionVariables.unset(\"names\");\r",
							"}\r",
							"\r",
							""
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"let names = pm.collectionVariables.get(\"names\");\r",
							"console.log(names);\r",
							"if (names === undefined || names.length === 0) {\r",
							"    console.log(\"initializing\");\r",
							"    names = [\"Test Board\", \"Test Board\", \"Assignment Board\"];\r",
							"} else {\r",
							"    console.log(names);\r",
							"}\r",
							"console.log(names);\r",
							"console.log(names.length);\r",
							"if (names != undefined && names.length > 0) {\r",
							"    const nextName = names.shift();\r",
							"    console.log(\"next name \" + nextName);\r",
							"    pm.collectionVariables.set(\"name\", nextName);\r",
							"    pm.collectionVariables.set(\"names\", names);\r",
							"}"
						],
						"type": "text/javascript"
					}
				}
			],
