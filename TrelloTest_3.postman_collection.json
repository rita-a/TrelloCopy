			"name": "Create a board",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"\r",
							"    // pm.collectionVariables.set(\"boardId\", pm.response.json().id);\r",
							"    // console.log(pm.response.json().id)\r",
							"    // console.log(pm.collectionVariables.get(\"boardId\"))\r",
							"    // console.log(\"trelloKey \" + pm.environment.get(\"trelloKey\"))    \r",
							"    // console.log(\"env \" + pm.environment.get(\"envName\"))    \r",
							"});\r",
							"\r",
							"var name = pm.variables.get(\"name\");\r",
							"pm.test(`Verify name ${name}`, function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.name).to.eql(`${name}`);\r",
							"    console.log(\"Response name \" + jsonData.name);\r",
							"    console.log(\"Requested name \" + `${name}`);\r",
							"    // if (pm.iterationData.get(\"verify\") === true) {\r",
							"    //     console.log(\"should be verified\");\r",
							"    // }\r",
							"});\r",
							"\r",
							"\r",
							"\r",
							""
